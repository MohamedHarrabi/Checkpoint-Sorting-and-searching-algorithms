//traitement
PROCEDURE  sort_cart_insertion(VAR tab : ARRAY_OF INTEGER)
BEGIN
FOR i FROM 1 TO n DO
   x:=tab[i];
   FOR j FROM i-1 TO n DO

      IF (x<tab[j]) THEN
       
        tab[i]:=tab[j];
        tab[j]=x;
        j:=i;
        break;
        END_IF
    END_FOR
    tab[j+1]=x;
END_FOR
END
 
